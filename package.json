{
  "name": "vrchat-group-guard",
  "private": true,
  "version": "1.0.10",
  "description": "A powerful VRChat group moderation and management tool",
  "author": "VRChat Group Guard Team",
  "license": "MIT",
  "main": "dist-electron/main.cjs",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/AppleExpl01t/Group-Guard-for-VRChat.git"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite --logLevel warn",
    "dev:electron": "wait-on tcp:5173 && esbuild electron/main.ts --bundle --platform=node --outfile=dist-electron/main.cjs --sourcemap --external:electron --external:vrchat && esbuild electron/preload.ts --bundle --platform=node --outfile=dist-electron/preload.cjs --sourcemap --external:electron && cross-env NODE_ENV=development electron .",
    "build": "tsc -b && vite build && npm run build:electron && electron-builder",
    "build:electron": "esbuild electron/main.ts --bundle --platform=node --outfile=dist-electron/main.cjs --minify --external:electron --external:vrchat && esbuild electron/preload.ts --bundle --platform=node --outfile=dist-electron/preload.cjs --minify --external:electron",
    "lint": "eslint .",
    "test": "vitest",
    "preview": "vite preview",
    "postinstall": "patch-package"
  },
  "build": {
    "appId": "com.vrchatgroupguard.app",
    "productName": "VRChat Group Guard",
    "copyright": "Copyright Â© 2024 VRChat Group Guard",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.png"
    },
    "nsis": {
      "artifactName": "VRChat.Group.Guard.Setup.${version}.exe"
    },
    "publish": [
      "github"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-fetch": "^4.1.0",
    "date-fns": "^4.1.0",
    "discord-rpc": "^4.0.1",
    "dotenv": "^17.2.3",
    "electron-log": "^5.4.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "framer-motion": "^12.23.26",
    "keyv-file": "^5.3.3",
    "lru-cache": "^11.2.5",
    "lucide-react": "^0.563.0",
    "node-osc": "^11.2.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-grid-layout": "^2.2.2",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "vrchat": "^2.20.7",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.0.10",
    "@types/node-osc": "^9.1.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-grid-layout": "^2.1.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.18",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^17.2.0",
    "jsdom": "^27.4.0",
    "patch-package": "^8.0.1",
    "prisma": "^5.11.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "wait-on": "^9.0.3"
  }
}
